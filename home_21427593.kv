<ScreenB>:
    userid:''
    name: 'screenB'
    Column:
        halign: 'center'
        valign: 'center'
        spacing: dp(20)

        canvas.before:
            Color:
                rgba: 255/255,250/255,250/255,1
            Rectangle:
                pos: self.pos
                size: self.size
                source: "firebase2.jpg"
        MDRaisedButton:
            text:'Welcome! User:'+ root.userid

        MDFillRoundFlatIconButton:
